{
  "firefly": {
    "limit": 1000
  },
  "identifyMethod": {
    "isracard": "identifier",
    "leumi": "hash",
    "visaCal": "identifier"
  },
  "currencySymbolMap": {
    "₪": "ILS",
    "$": "USD",
    "€": "EUR"
  },
  "creditCardDesc": [
    {
      "desc": "ל.מאסטרקרד(יש)",
      "creditCard": "isracard"
    },
    {
      "desc": "ישראכרט-י",
      "creditCard": "isracard"
    },
    {
      "desc": "ויזה כ.א.ל-י",
      "creditCard": "visaCal"
    },
    {
      "desc": "כרטיסי אשראי-י",
      "creditCard": "visaCal"
    },
    {
      "desc": "מקס איט פיננ-י",
      "creditCard": "max"
    }
  ],
  "scraper": {
    "parallel": true,
    "options": {
      "args": [],
      "combineInstallments": false,
      "showBrowser": false,
      "outputData": {
        "enableTransactionsFilterByDate": false
      },
      "additionalTransactionInformation": true
    }
  },
  "log": {
    "level": "info",
    "prettyPrint": true,
    "redact": [
      "error.config.headers.Authorization",
      "banks[*].credentials",
      "banks[*].credentials.username",
      "banks[*].credentials.password",
      "banks[*].credentials.id",
      "banks[*].credentials.userCode",
      "banks[*].credentials.card6Digits",
      "banks[*].creditCards[*].credentials",
      "banks[*].creditCards[*].credentials.username",
      "banks[*].creditCards[*].credentials.password",
      "banks[*].creditCards[*].credentials.id",
      "banks[*].creditCards[*].credentials.card6Digits",
      "firefly.tokenApi",
      "*.password",
      "*.credentials",
      "*.tokenApi",
      "*.token",
      "*.secret",
      "*.apiKey",
      "error.*.password",
      "error.*.credentials",
      "error.*.tokenApi"
    ]
  }
}
